{
  "name": "edofre/laravel-fullcalendar-scheduler",
  "description": "Laravel component for fullcalendar scheduler module",
  "keywords": [
    "fullcalendar-scheduler",
    "scheduler",
    "javascript",
    "event",
    "calendar",
    "laravel"
  ],
  "homepage": "https://github.com/edofre/laravel-fullcalendar-scheduler",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Freriks",
      "email": "edofre@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.*",
    "npm-asset/fullcalendar": "v3.8.0",
    "npm-asset/fullcalendar-scheduler": "1.9.1"
  },
  "require-dev": {
    "orchestra/testbench": "~3.2.0|~3.3.0",
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Edofre\\FullcalendarScheduler\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Edofre\\FullcalendarScheduler\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Edofre\\FullcalendarScheduler\\FullcalendarSchedulerServiceProvider"
      ],
      "aliases": {
        "FullcalendarScheduler": "Edofre\\FullcalendarScheduler\\Facades\\FullcalendarScheduler"
      }
    }
  }
}